{
  "manifest_version": 3,
  "name": "Nyan Scroll",
  "version": "1.1",
  "description": "Customize your browser's scrollbar with a fun Nyan Cat theme.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ]
}
